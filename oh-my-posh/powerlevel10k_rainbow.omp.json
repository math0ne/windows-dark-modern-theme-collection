{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#CCCCCC",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "#569cd6",
          "style": "plain",
          "template": "\u2591\u2592\u2593",
          "type": "text"
        },
        {
          "background": "#569cd6",
          "foreground": "#1F1F1F",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#4EC9B0",
          "foreground": "#1F1F1F",
          "powerline_symbol": "\ue0b0",
          "options": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "powerline",
          "template": " \uf07c {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#d7ba7d",
          "foreground": "#1F1F1F",
          "powerline_symbol": "\ue0b0",
          "options": {
            "branch_icon": "\uf126 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD | trunc 15 }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#DCDCAA",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "type": "node"
        },
        {
          "background": "#9CDCFE",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "type": "go"
        },
        {
          "background": "#C586C0",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624 ",
          "type": "julia"
        },
        {
          "background": "#b5cea8",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "type": "python"
        },
        {
          "background": "#ce9178",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "type": "ruby"
        },
        {
          "background": "#6A9955",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#d7ba7d{{end}}",
            "{{if contains \"jan\" .Profile}}#ce9178{{end}}"
          ],
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "options": {
            "display_default": false
          },
          "style": "powerline",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad ",
          "type": "aws"
        },
        {
          "background": "#DCDCAA",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "background": "#9cdcfe",
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "#6A9955",
          "background_templates": [
            "{{ if gt .Code 0 }}#F85149{{ end }}"
          ],
          "foreground": "#1F1F1F",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "foreground": "#569cd6",
          "style": "plain",
          "template": "\ue0b2",
          "type": "text"
        },
        {
          "foreground": "#569cd6",
          "style": "plain",
          "template": "\u2593\u2592\u2591",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#CCCCCC",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 4
}